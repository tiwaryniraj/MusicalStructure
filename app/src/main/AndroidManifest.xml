<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.niraj.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PlaylistActivity"
            android:label="PlayLists"/>
        <activity android:name=".ArtistActivity"
            android:label="Artists"/>
        <activity android:name=".AlbumActivity"
            android:label="Album"/>
        <activity android:name=".SongActivity"
            android:label="Songs"/>
        <activity android:name=".PlayingActivity"
            android:label="Now Playing!"></activity>
    </application>

</manifest>